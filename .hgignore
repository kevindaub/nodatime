syntax:glob

# Editor backups and temporary files
*~
*.bak
*.swp

# IDE-generated files
*.suo
*.user
*.sln.cache

# IDE add-ins
*.dotCover
*.ncrunchproject
*.ncrunchsolution
_NCrunch*    # cache directories
.resharper   # solution
*.ReSharper  # project
_ReSharper*  # directories
StyleCop.Cache

# Auto-created thumbnails
Thumbs.db

# javac output
*.class

# Joda Time jar files
# (It's reasonably easy to end up with one in a working directory...)
joda-time*.jar

# Noda Time release (private) key
NodaTime Release.snk

# Need to use regexp syntax for rooted patterns and directory matches.
syntax:regexp

# Build output directories (anywhere)
/bin/
/obj/

# NUnit output
^TestResult\.xml$

# The docs directory - we've already got docs/api/Index.html, but we don't want anything new.
^docs/

# Sandcastle per-user settings
^NodaTime.shfbproj_.+$
^NodaTime-Pcl.shfbproj_.+$

# NuGet temporary build area (see buildnuget.bat)
^nuget/

# Generated web site
^www/_site/

# Temporary build output directories
^tmpdocs
^tmpwebdir
^tzdbtmp
^SandcastleStyleTmp

# NuGet packages (but not the configuration)
^src/packages/.*/

# Local copy of benchmark data and Mercurial log, used for testing the dynamic part of the web site
^src/NodaTime.Web/benchmarkdata/
^src/NodaTime.Web/hg-log.xml

# Roslyn temporary files
^src/.*\.sln\.ide/

# Generated packages/projects
^src/NodaTime-Core-.*.sln$
^src/.*/.*-.*.csproj$
