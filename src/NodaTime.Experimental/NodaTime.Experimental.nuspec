<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>NodaTime.Experimental</id>
    <version>0.1.0</version>
    <authors>Jon Skeet</authors>
    <owners>Jon Skeet</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>http://noda-time.googlecode.com</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Provides experimental extensions to the Noda Time. Functionality in
      this package may be removed or changed without warning - it
      is designed to be a way of experimenting with different
      designs so that multiple approaches can be tried in parallel in
      the same codebase (rather than in separate branches which can be
      hard to compare directly).
    </description>
    <tags>nodatime api</tags>
    <dependencies>
      <dependency id="NodaTime" version="0.1" />      
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\NodaTime.Experimental.dll" target="lib" />
    <file src="bin\Release\NodaTime.Experimental.xml" target="lib" />
  </files>
</package>
    